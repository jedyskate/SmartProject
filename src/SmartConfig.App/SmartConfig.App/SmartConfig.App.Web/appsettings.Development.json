{
  "SmartConfig":{
    "ApiEndpoint": "https://localhost:5111",
    "AgentEndpoint": "https://localhost:7153",
    "n8nEndpoint": "http://localhost:5678"
  },
  "RabbitMq": {
    "Enabled": true,
    "TcpEndpoint": {
      "HostName": "localhost",
      "Port": 5672
    },
    "Credentials": {
      "UserName": "admin",
      "Password": "admin123",
      "VirtualHost": "/"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "apiRoute": {
        "ClusterId": "backendCluster",
        "Match": {
          "Path": "/api/{**path}"
        }
      },
      "agentRoute": {
        "ClusterId": "agentCluster",
        "Match": {
          "Path": "/agent/{**path}"
        }
      }
    },
    "Clusters": {
      "backendCluster": {
        "Destinations": {
          "main-destination": {
            "Address": "https://localhost:5111/"
          }
        }
      },
      "agentCluster": {
        "Destinations": {
          "agent-destination": {
            "Address": "https://localhost:7153/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.SignalR": "Debug"
    }
  },
  "DetailedErrors": true
}
