{
  "SmartConfig":{
    "ApiEndpoint": "https://localhost:5111",
    "n8nEndpoint": "http://localhost:5678"
  },
  "RabbitMq": {
    "Enabled": true,
    "TcpEndpoint": {
      "HostName": "localhost",
      "Port": 5672
    },
    "Credentials": {
      "UserName": "admin",
      "Password": "admin123",
      "VirtualHost": "/"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "apiRoute": {
        "ClusterId": "backendCluster", // Link this route to the 'backendCluster' defined below.
        "Match": {
          "Path": "/api/{**path}"
        }
      }
    },
    "Clusters": {
      "backendCluster": {
        "Destinations": {
          "main-destination": {
            "Address": "https://localhost:5111"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.SignalR": "Debug"
    }
  },
  "DetailedErrors": true
}
